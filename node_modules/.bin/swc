#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/bin/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules:/home/brian/work/ctx-core/node_modules:/home/brian/work/node_modules:/home/brian/node_modules:/home/node_modules:/node_modules:/home/brian/work/ctx-core/ctx-core/packages/ripple-effect-ui-svelte/node_modules:/home/brian/work/ctx-core/ctx-core/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/bin/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules/.pnpm/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules:/home/brian/work/ctx-core/node_modules:/home/brian/work/node_modules:/home/brian/node_modules:/home/node_modules:/node_modules:/home/brian/work/ctx-core/ctx-core/packages/ripple-effect-ui-svelte/node_modules:/home/brian/work/ctx-core/ctx-core/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/bin/swc.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@swc+cli@0.1.55_@swc+core@1.2.120/node_modules/@swc/cli/bin/swc.js" "$@"
fi
